/ {
    macros {
        eszett_macro: eszett_macro {
            compatible = "zmk,behavior-macro";
            wait-ms = <40>;
            tap-ms = <40>;
            bindings = <
                &macro_press   &kp LALT
                &macro_tap     &kp N0 &kp N2 &kp N2 &kp N3
                &macro_release &kp LALT
            >;
        };

        ue_macro: ue_macro {
